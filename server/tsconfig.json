{
  "compilerOptions": {
    "target": "ES2020",
     "typeRoots": ["./src/types", "./node_modules/@types"] ,
    "module": "CommonJS",
    "outDir": "./dist",
    "resolveJsonModule": true,
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}